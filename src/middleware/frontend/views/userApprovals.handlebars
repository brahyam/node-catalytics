<div class="container">
    <div class="row">
        <h1>Pending Approvals</h1>
        <div class="col-md-6">
            <table class="table table-striped">
                <tr>
                    <th>#</th>
                    <th>Email</th>
                    <th>Approve</th>
                </tr>
              {{#each users}}
                  <tr>
                      <td>
                        {{id}}
                      </td>
                      <td>
                        {{email}}
                      </td>
                      <td class="text-center">
                          <a href="/ui/settings/approvals/{{id}}" class="text-black"><i class="fa fa-check"></i></a>
                      </td>
                  </tr>
              {{/each}}
            </table>
        </div>
    </div>
</div>

